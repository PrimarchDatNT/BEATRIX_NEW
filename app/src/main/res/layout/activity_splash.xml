<?xml version="1.0" encoding="utf-8"?>
<com.meitu.ratiorelativelayout.RatioRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@id/rl_root"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    app:layout_heightSpec="1920.0"
    app:layout_widthSpec="1080.0">
    <com.meitu.hwbusinesskit.core.widget.NativeAdView
        android:id="@id/nativeAdView"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="@android:color/white"
        android:clipChildren="false"
        android:visibility="gone"
        app:mthwb_advertLayout="@layout/layout_launch_ads" />
    <RelativeLayout
        android:id="@id/rl_start_ad_skip_in_top"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:paddingLeft="50.0dip"
        android:paddingBottom="50.0dip"
        android:visibility="gone">
        <TextView
            android:id="@id/tv_start_ad_skip_in_top"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15.0dip"
            android:layout_marginRight="10.0dip"
            android:background="@drawable/startup_skip_selector"
            android:gravity="center"
            android:paddingStart="15.0dip"
            android:paddingLeft="15.0dip"
            android:paddingTop="5.0dip"
            android:paddingEnd="15.0dip"
            android:paddingRight="15.0dip"
            android:paddingBottom="5.0dip"
            android:text="@string/startup_advert_skip"
            android:textColor="@color/white"
            android:textSize="14.0sp" />
    </RelativeLayout>
    <RelativeLayout
        android:id="@id/rl_start_ad_skip_in_bottom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:paddingLeft="50.0dip"
        android:paddingTop="50.0dip"
        android:visibility="gone"
        app:layout_ratioMarginBottom="315.0">
        <TextView
            android:id="@id/tv_start_ad_skip_in_bottom"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="10.0dip"
            android:layout_marginBottom="15.0dip"
            android:background="@drawable/startup_skip_selector"
            android:gravity="center"
            android:paddingStart="15.0dip"
            android:paddingLeft="15.0dip"
            android:paddingTop="5.0dip"
            android:paddingEnd="15.0dip"
            android:paddingRight="15.0dip"
            android:paddingBottom="5.0dip"
            android:text="@string/startup_advert_skip"
            android:textColor="@color/white"
            android:textSize="14.0sp" />
    </RelativeLayout>
</com.meitu.ratiorelativelayout.RatioRelativeLayout>