<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@id/rl_parent"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:tag="layout/adsorbent_layout_0">
    <com.commsource.widget.LinearGradient
        android:id="@id/mGradientView"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent" />
    <ImageView
        android:id="@id/iv_diamond"
        android:layout_width="189.0dip"
        android:layout_height="88.0dip"
        android:layout_gravity="bottom|center|right"
        android:alpha="1.0"
        android:scaleType="fitEnd"
        android:src="@drawable/premium_banner_img_diamond" />
    <com.commsource.beautymain.widget.RoundCornerFrameLayout
        android:id="@id/vRoundCornerContainer"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:visibility="gone"
        app:layoutCornerRadius="6.0dip">
        <ImageView
            android:id="@id/vIvOnlineBg"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_gravity="bottom|center" />
    </com.commsource.beautymain.widget.RoundCornerFrameLayout>
    <RelativeLayout
        android:id="@id/rl_content"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_gravity="bottom">
        <com.commsource.widget.IconFrontView
            android:layout_width="35.0dip"
            android:layout_height="35.0dip"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="16.0dip"
            android:gravity="center"
            android:text="@string/if_arrow_right"
            android:textColor="@color/white"
            android:textSize="35.0dip" />
        <FrameLayout
            android:id="@id/vFlCountDownContainer"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="51.0dip"
            android:tag="binding_1"
            android:visibility="gone">
            <TextView
                android:id="@id/tv_count_down"
                android:layout_width="wrap_content"
                android:layout_height="22.0dip"
                android:layout_gravity="center"
                android:gravity="center"
                android:textColor="@color/white"
                android:textSize="12.0dip"
                android:textStyle="bold" />
        </FrameLayout>
        <RelativeLayout
            android:id="@id/rl_subs_no_off"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="16.0dip"
            android:layout_marginRight="45.0dip"
            android:orientation="vertical">
            <com.commsource.widget.GradientTextView
                android:id="@id/tv_no_off_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:maxLines="1"
                android:text="@string/beauty_plus_premium"
                android:textColor="@color/white"
                android:textSize="18.0dip" />
            <com.commsource.widget.BoldTextView
                android:id="@id/tv_no_off_free_try"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:maxLines="1"
                android:text="@string/sub_7_day_try"
                android:textColor="@color/white"
                android:textSize="17.0dip"
                android:visibility="gone" />
            <TextView
                android:id="@id/tv_no_off_free_try_subtitle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/tv_no_off_free_try"
                android:layout_marginTop="-3.0dip"
                android:text="@string/beauty_plus_premium"
                android:textColor="@color/white"
                android:textSize="13.0dip"
                android:visibility="gone" />
        </RelativeLayout>
        <LinearLayout
            android:id="@id/ll_subs_off"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="16.0dip"
            android:orientation="vertical"
            android:visibility="gone">
            <com.commsource.widget.GradientTextView
                android:id="@id/tv_subs_off"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="45% off"
                android:textColor="@color/white"
                android:textSize="17.0dip" />
            <com.commsource.widget.AutoFitTextView
                android:id="@id/tv_subs_off_bottom"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:maxLines="1"
                android:text="@string/beauty_plus_premium"
                android:textColor="@color/white"
                android:textSize="13.0dip" />
        </LinearLayout>
        <com.commsource.beautymain.widget.RoundCornerFrameLayout
            android:id="@id/vRcPicTitleContainer"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_centerVertical="true"
            android:layout_marginEnd="16.0dip"
            android:layout_toStartOf="@id/vFlCountDownContainer"
            android:visibility="gone"
            app:layoutCornerRadius="6.0dip">
            <ImageView
                android:id="@id/vIvPicTitle"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:scaleType="fitStart" />
        </com.commsource.beautymain.widget.RoundCornerFrameLayout>
    </RelativeLayout>
    <View
        android:id="@id/ivMask"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="@drawable/ripple_radius_6" />
</FrameLayout>