<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@color/white"
    android:clickable="true"
    android:focusable="true"
    android:tag="layout/fragment_sticker_manager_0">
    <androidx.recyclerview.widget.RecyclerView
        android:id="@id/stickerGroupRv"
        android:layout_width="fill_parent"
        android:layout_height="0.0dip"
        android:background="@color/white"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@id/divider"
        app:layout_constraintVertical_weight="1.0" />
    <View
        android:id="@id/title_cover"
        android:layout_width="fill_parent"
        android:layout_height="57.0dip"
        android:background="@color/white"
        android:clickable="true"
        android:focusable="true"
        app:layout_constraintTop_toTopOf="parent" />
    <com.commsource.widget.IconFrontView
        android:id="@id/back"
        android:layout_width="35.0dip"
        android:layout_height="35.0dip"
        android:layout_marginStart="16.0dip"
        android:layout_marginTop="11.0dip"
        android:gravity="center"
        android:text="@string/if_left_camera"
        android:textColor="@color/black"
        android:textSize="35.0dip"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <TextView
        android:id="@id/title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/stick_manager"
        android:textColor="@color/black"
        android:textSize="16.0dip"
        app:layout_constraintBottom_toBottomOf="@id/back"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@id/back" />
    <View
        android:id="@id/divider"
        android:layout_width="fill_parent"
        android:layout_height="2.0px"
        android:layout_marginTop="11.0dip"
        android:background="@color/color_cccccc"
        app:layout_constraintTop_toBottomOf="@id/back" />
    <include
        android:id="@id/manager_prompt_layout"
        layout="@layout/layout_manager_prompt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="29.0dip"
        android:layout_marginRight="7.0dip"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/divider" />
    <View
        android:id="@id/barrier"
        android:layout_width="fill_parent"
        android:layout_height="0.0dip"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@id/divider"
        app:layout_constraintVertical_weight="1.0" />
</androidx.constraintlayout.widget.ConstraintLayout>